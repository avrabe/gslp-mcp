/// Production-Grade Object Detection AI Component World  
/// Demonstrates real automotive AI with WASI-NN and safety compliance
package showcase:object-detection;

world object-detection {
    // ============ IMPORTS ============
    // Standard automotive orchestration for FEO compliance
    import adas:orchestration/execution-control;
    import adas:orchestration/resource-management;
    import adas:orchestration/data-flow;
    
    // Complete automotive data type system
    import adas:common-types/types;
    import adas:common-types/sensor-fusion;
    
    // Input: Production automotive sensor data
    import adas:data/sensor-data;
    
    // Output: Production automotive perception data  
    import adas:data/perception-data;
    
    // Production WASI-NN for automotive AI inference (v0.2.0-rc-2024-10-28)
    import wasi:nn/graph@0.2.0-rc-2024-10-28;
    import wasi:nn/tensor@0.2.0-rc-2024-10-28;
    import wasi:nn/inference@0.2.0-rc-2024-10-28;
    import wasi:nn/errors@0.2.0-rc-2024-10-28;
    
    // File system for embedded production ONNX models
    import wasi:filesystem/types@0.2.0;
    import wasi:filesystem/preopens@0.2.0;
    
    // Automotive timing and safety systems
    import wasi:clocks/wall-clock@0.2.0;
    import wasi:clocks/monotonic-clock@0.2.0;
    
    // ============ EXPORTS ============
    // Production AI control interface (automotive-grade model management)
    export adas:control/ai-control;
    
    // Production perception data output (detected objects with automotive metadata)
    export adas:data/perception-data;
    
    // Automotive health monitoring (ASIL-B safety requirements)
    export adas:diagnostics/health-monitoring;
    
    // Real-time performance monitoring (automotive timing constraints)
    export adas:diagnostics/performance-monitoring;
    
    // AI processing interface for showcase integration
    export showcase:ai/object-processor;
}